# include<stdio.h>
int main()
{
    int m, n;
    
    printf("enter the value of M : ");
    scanf("%d", &m);
    
    printf("enter the value of N: ");
    scanf("%d", &n);
    
    for (int i = m; i <= n; i++){
        if (i % 2 == 0){
            printf("Even numbers between %d and %d are: ", m, n);
            printf("%d", i);
        }
    }
    
    printf("\n Odd numbers between %d and %d are: ", m ,n);
    
    for(int i = m; i <= n; i++){
        if(i % 2 != 0){
            printf("%d\n", i);
        }
    }
    return 0;
}
