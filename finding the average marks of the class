#include <stdio.h>

int main() {
int m[5],i,sum = 0, n;
float avg;
printf("enter number of the students:\n ");
scanf("%d", &n);
printf("enter the makrks of the students:\n");
scanf("%d", &m[5]);
for(i = 0; i <= n; i++)
scanf("%d", &m[i]);
for(i = 0; i <= n; i++)
sum = sum+m[i];
avg = (float)sum/n;
printf("average = %f", avg);
for(i = 0; i < n; i++)
printf("marks are = %d\n", m[i]);


    return 0;
}
